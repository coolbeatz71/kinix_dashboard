            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run type:check && lint-staged",
            "pre-push": "enforce-branch-name '^(ft|ch|bg)-[a-z]+(-[a-z]+)*$'"
        }
    },
    "lint-staged": {
        "*.+(jsx|js|json|css|scss|md|tsx|ts)": [
            "prettier --write"
        ],
        "*.+(js|jsx|tsx|ts)": [
            "eslint --fix"
        ]
